@using Microsoft.Extensions.Options
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@model IEnumerable<DubrovnikTours.Web.Models.Home.TourCollectionByTypeViewModel>
@inject IOptions<MarioTravel.Core.Configuration.Api.Keys.GoogleMaps> GoogleMaps


    <div class="page page-home">
        <div id="myCarousel" class="carousel carousel-fade slide" data-ride="carousel" data-interval="8000">
            <div class="carousel-inner">
                <a class="carousel-control-prev" data-target="#myCarousel" onclick="return false" role="button" data-slide="prev">
                    <svg class="carousel-arrow carousel-arrow-prev"
                         width="52"
                         height="52"
                         viewBox="0 0 52 52"
                         fill="transparent"
                         xmlns="http://www.w3.org/2000/svg">
                        <circle r="25" transform="matrix(-1 0 0 1 26 26)" stroke="white" fill="transparent" stroke-width="2" />
                        <path d="M38 29H16L26.3459 18" stroke="white" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </a>
                <a class="carousel-control-next" data-target="#myCarousel" onclick="return false" role="button" data-slide="next">
                    <svg class="carousel-arrow carousel-arrow-next carousel-control-next" data-slide="next"
                         width="52"
                         height="52"
                         viewBox="0 0 52 52"
                         fill="transparent"
                         xmlns="http://www.w3.org/2000/svg">
                        <circle cx="26" cy="26" r="25" stroke="white" stroke-width="2" />
                        <path d="M14 29H36L25.6541 18" stroke="white" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </a>
                    @foreach (var tourCollection in Model)
                    {
                        if (tourCollection.Tours.Any())
                        {
                            @foreach (var tour in tourCollection.Tours)
                            {
                                if (tour.LanguageId == 1)
                                {
                                    int tourId = tour.Id;
                                    switch (tourId)
                                    {
                                        case 39:
                                            <div class="item active background a">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                        case 40:
                                            <div class="item background b">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                        case 41:
                                            <div class="item background c">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                    }

                                }
                                else if (tour.LanguageId == 2)
                                {
                                    int tourId = tour.Id;
                                    switch (tourId)
                                    {
                                        case 42:
                                            <div class="item active background a">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                        case 43:
                                            <div class="item background b">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                        case 44:
                                            <div class="item background c">
                                                <div class="covertext">
                                                    <h3 class="hero-slider-slide-title">@tour.Title<span class="hero-slider-slide-title-ghost">DISCOVER</span></h3>
                                                    <p class="hero-slider-slide-description">@tour.Description</p>
                                                    <div class="hero-slider-slide-buttons">
                                                        <a asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" class="hero-slider-slide-buttons-btn btn-primary">@Localizer["Book Online"]</a>
                                                    </div>
                                                </div>
                                            </div>
                                            break;
                                    }
                                }

                            }
                        }
                    }
            </div>
        </div>
        <div class="tours">
            <div class="tours-tabs">
                @foreach (var tourCollection in Model)
                        {
                            if (tourCollection.Tours.Any())
                            {
                                @if (tourCollection.Type == "Scheduled")
                                {
                            <a class="tours-tabs-tab is-active" href="#tours" onclick="return false" data-show-tab="@tourCollection.Type.ToLower()">
