                                    <path d="M35 40H5C4.34339 40 3.69321 39.8707 3.08658 39.6194C2.47995 39.3681 1.92876 38.9998 1.46447 38.5355C1.00017 38.0712 0.631876 37.52 0.380602 36.9134C0.129329 36.3068 0 35.6566 0 35L0 5C0 3.67392 0.526784 2.40215 1.46447 1.46447C2.40215 0.526784 3.67392 0 5 0H35C36.3261 0 37.5979 0.526784 38.5355 1.46447C39.4732 2.40215 40 3.67392 40 5V35C40 35.6566 39.8707 36.3068 39.6194 36.9134C39.3681 37.52 38.9998 38.0712 38.5355 38.5355C38.0712 38.9998 37.52 39.3681 36.9134 39.6194C36.3068 39.8707 35.6566 40 35 40ZM5 1.25C4.00625 1.25264 3.05396 1.64858 2.35127 2.35127C1.64858 3.05396 1.25264 4.00625 1.25 5V35C1.25 35.9946 1.64509 36.9484 2.34835 37.6517C3.05161 38.3549 4.00544 38.75 5 38.75H35C35.9946 38.75 36.9484 38.3549 37.6516 37.6517C38.3549 36.9484 38.75 35.9946 38.75 35V5C38.7474 4.00625 38.3514 3.05396 37.6487 2.35127C36.946 1.64858 35.9938 1.25264 35 1.25H5Z" fill="white" />
                                    <path d="M31.9923 17.5716C32.0026 17.4961 32.0026 17.4195 31.9923 17.344C31.9436 17.2115 31.8599 17.0946 31.7501 17.0058C31.6403 16.917 31.5085 16.8595 31.3686 16.8396L24.0034 15.8504L20.6673 9.40091C20.5995 9.27936 20.5004 9.17812 20.3804 9.10764C20.2603 9.03716 20.1235 9 19.9843 9C19.845 9 19.7083 9.03716 19.5882 9.10764C19.4681 9.17812 19.369 9.27936 19.3012 9.40091L16.0245 15.8108L8.65934 16.8C8.51834 16.8167 8.38495 16.8729 8.27459 16.9622C8.16422 17.0515 8.0814 17.1701 8.03568 17.3045C7.99267 17.4371 7.98845 17.5792 8.02353 17.7142C8.0586 17.8491 8.13151 17.9712 8.23367 18.0661L13.5596 23.0121L12.3023 30.055C12.2776 30.1936 12.2934 30.3364 12.3479 30.4662C12.4023 30.596 12.493 30.7074 12.6092 30.787C12.7417 30.8742 12.8961 30.9222 13.0547 30.9255C13.1791 30.9266 13.3018 30.8959 13.4111 30.8365L19.9843 27.592L26.5773 30.9156C26.7035 30.9786 26.8441 31.0072 26.9849 30.9985C27.1256 30.9898 27.2616 30.9442 27.3791 30.8662C27.4953 30.7866 27.586 30.6752 27.6405 30.5453C27.6949 30.4155 27.7107 30.2727 27.686 30.1342L26.4288 23.0912L31.7547 18.1453C31.8324 18.0717 31.8937 17.9826 31.9346 17.8837C31.9756 17.7849 31.9952 17.6785 31.9923 17.5716ZM15.1633 22.9626C15.1847 22.8419 15.1751 22.7177 15.1353 22.6018C15.0954 22.4858 15.0267 22.3819 14.9356 22.2998L10.4412 18.0958L16.6581 17.2253C16.7798 17.2065 16.8957 17.1601 16.9969 17.0899C17.0981 17.0196 17.182 16.9272 17.2421 16.8198L19.9843 11.3991L22.766 16.8198C22.8262 16.9272 22.91 17.0196 23.0112 17.0899C23.1124 17.1601 23.2283 17.2065 23.3501 17.2253L29.5669 18.0958L25.0726 22.2998C24.9822 22.3825 24.914 22.4864 24.8742 22.6022C24.8344 22.718 24.8243 22.8419 24.8449 22.9626L25.914 28.8977L20.3407 26.0785C20.2291 26.0264 20.1074 25.9994 19.9843 25.9994C19.8611 25.9994 19.7395 26.0264 19.6279 26.0785L14.0446 28.9175L15.1633 22.9626Z" fill="white" />
                                </svg>
                                <span class="tours-tabs-tab-text">@Localizer[@tourCollection.Type.ToUpper()]</span>
                            </a>
                        }
                    }
                }
            </div>
            <div id="tours"></div>
            <div class="container">
                <div id="home-fix"></div>
                @{
                    var isFirst = true;
                }

                @foreach (var tourCollection in Model)
                        {
                            if (tourCollection.Tours.Any())
                            {
                                var tourType = tourCollection.Type.ToLowerInvariant();
                        <div class="tours-cards" data-tab="@tourType" @{if (!isFirst) { <text> style="display: none;" </text>  } }>
                            <svg id="@tourType-arrow-prev" class="tours-slider-arrow tours-slider-arrow-prev" width="52" height="52" viewBox="0 0 52 52" fill="transparent" xmlns="http://www.w3.org/2000/svg">
                                <circle r="25" transform="matrix(-1 0 0 1 26 26)" stroke="white" fill="transparent" stroke-width="2" />
                                <path d="M38 29H16L26.3459 18" stroke="white" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" />
                            </svg>
                            <svg id="@tourType-arrow-next" class="tours-slider-arrow tours-slider-arrow-next" width="52" height="52" viewBox="0 0 52 52" fill="transparent" xmlns="http://www.w3.org/2000/svg">
                                <circle cx="26" cy="26" r="25" stroke="white" stroke-width="2" />
                                <path d="M14 29H36L25.6541 18" stroke="white" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" />
                            </svg>
                            <div id="@tourType-slider" class="tours-cards-slider">
                                @foreach (var tour in tourCollection.Tours)
                        {
                                    <div class="card">
                                        <a class="card-link" asp-controller="Tour" asp-action="TourBooking" asp-route-id="@tour.Id" asp-route-slug="@tour.Slug" aria-hidden="true"></a>
                                        <div class="card-discount">
                                            <span>SAVE</span>
                                            <strong>@(tour.DiscountPercent)%</strong>
                                        </div>
                                        <img class="card-image lazy" src="/ico/blank.svg" data-original="@Url.Content(tour.MainImage.Url)" alt="@tour.MainImage.AltDescription" />
                                        <div class="card-body fade-out">
                                            <span class="card-title">@tour.Title</span>
                                            <span class="card-category">@Localizer[@tourCollection.Type]</span>
                                            <p class="card-description">@tour.Description</p>
                                        </div>

                                        <div class="card-info">
                                            <span class="card-duration">@Localizer["Duration"].Value.ToUpper():</span><span class="card-duration">&nbsp; @tour.DisplayDuration @Localizer["hours"]</span>
                                            <div class="card-footer">
                                                <span class="card-price">@tour.DisplayPrice </span>
                                                <svg class="card-footer-icon card-footer-see-more" width="38" height="7" viewBox="0 0 38 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <path d="M1 6.5H37L31 1" stroke="#18b0d0" stroke-linecap="round" stroke-linejoin="round" />
                                                </svg>
                                                <svg class="card-footer-icon card-footer-schedule" width="30" height="30" viewBox="0 0 30 29" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <path d="M22.3318 13.0874V5.28155C22.3318 3.88349 21.1811 2.7767 19.7944 2.7767H16.6374V1.90291C16.6374 1.40777 16.2243 1 15.7228 1C15.2212 1 14.8081 1.40777 14.8081 1.90291V2.74757H8.52365V1.90291C8.52365 1.40777 8.11059 1 7.60901 1C7.10743 1 6.69437 1.40777 6.69437 1.90291V2.74757H3.53739C2.12117 2.74757 1 3.8835 1 5.25243V18.8835C1 20.2816 2.15068 21.3883 3.53739 21.3883H13.8935C14.336 25.0874 17.552 28 21.4171 28C25.6068 28 28.9998 24.6505 28.9998 20.5146C29.0293 16.699 26.1083 13.5243 22.3318 13.0874ZM3.53739 4.5534H6.69437V5.39806C6.69437 5.8932 7.10743 6.30097 7.60901 6.30097C8.11059 6.30097 8.52365 5.8932 8.52365 5.39806V4.5534H14.8081V5.39806C14.8081 5.8932 15.2212 6.30097 15.7228 6.30097C16.2243 6.30097 16.6374 5.8932 16.6374 5.39806V4.5534H19.7944C20.2074 4.5534 20.532 4.87379 20.532 5.28155V8.33981H2.82928V5.28155C2.82928 4.87379 3.15383 4.5534 3.53739 4.5534ZM2.82928 18.9126V10.1165H20.532V13.0874C17.08 13.4951 14.336 16.2039 13.923 19.6116H3.53739C3.15383 19.6116 2.82928 19.2913 2.82928 18.9126ZM21.4466 26.2233C18.2601 26.2233 15.6637 23.6602 15.6637 20.5146C15.6637 17.3689 18.2601 14.835 21.4466 14.835C24.6331 14.835 27.2295 17.3981 27.2295 20.5437C27.2295 23.6893 24.6331 26.2233 21.4466 26.2233Z" fill="white" stroke="white" stroke-width="0.75" />
